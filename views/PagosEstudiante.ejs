<h2 class="pb-4" >Pagos del Estudiante</h2>
<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3 mb-4">
    <table >
        <thead>
            <tr>
                <th>Carnet</th>
                <th>Monto Pagado</th>
                <th>No. de Meses pagados</th>
                <th>Tipo de pago</th>
            </tr>
        </thead>
        <tbody>
            <% if(pagos != '') { %>
                <% pagos.forEach(function(pago, index) { %>
                    <tr>
                        <td><%- pago.estudiantecarnet %></td>
                        <td><%- pago.total %></td>
                        <td><%- pago.meses %></td>
                        <td><%- pago.tipo %></td>
                    </tr>
                <% }) %>
            <% } %>
        </tbody>
    </table>
    
</div>